<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleTest </type>
        </aboveground_competition>
        <belowground_competition>
            <type> SimpleTest </type>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleTest </type>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Recruiting </name>
            <species> Avicennia </species>
            <distribution>
                <type> GroupFromFile </type>
                <filename> test/SmallTests/Test_Setups_small/input_population/InputPopulation.csv </filename>
                <n_recruitment_per_step> 0 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> GroupFromFile </type>
		<filename> test/SmallTests/Test_Setups_small/initial_population/InitialPopulation.csv </filename>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 1e9 </t_end>
        <delta_t> 1e7 </delta_t>
    </tree_time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <tree_output>
        <type> OneTreeOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> test/testoutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <parameter_output> k_geom </parameter_output>
    </tree_output>
</MangaProject>
