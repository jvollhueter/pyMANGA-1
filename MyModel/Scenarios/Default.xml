<MangaProject>
    <resources>
        <aboveground>
            <type> AsymmetricZOI </type>
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 100 </x_2>
                <y_2> 10 </y_2>
            <x_resolution> 100 </x_resolution>
            <y_resolution> 100 </y_resolution>
            </domain>
        </aboveground>
        <belowground>
            <type> FixedSalinity </type>
            <variant> forman </variant>
            <min_x>0</min_x>
            <max_x>100</max_x>
            <salinity>0.035 0.120</salinity>
        </belowground>
    </resources>
    <population>
        <group>
            <name> Saltmarsh_1 </name>
            <species> PopulationLib/Species/Saltmarsh_1.py </species>
            <vegetation_model_type> Saltmarsh </vegetation_model_type>
            <mortality> Threshold </mortality>
            <threshold_type> biovolume </threshold_type>
            <threshold> 1.5707963267948967e-06 </threshold>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 100 </x_2>
                    <y_2> 10 </y_2>
                </domain>
                <n_recruitment_per_step> 20 </n_recruitment_per_step>
                <n_individuals> 50 </n_individuals>
            </distribution>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 3.2e+7 </t_end>
        <delta_t> 86400 </delta_t>
    </time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <output>
        <type> OnePlantOneFile </type>
        <output_time_range > [0, 3.2e+7] </output_time_range >
        <allow_previous_output>True</allow_previous_output>
        <output_dir>MyModel/Outputs/default</output_dir>
        <geometry_output> r_ag </geometry_output>
        <geometry_output> h_ag </geometry_output>
        <geometry_output> r_bg </geometry_output>
        <geometry_output> h_bg </geometry_output>
        <growth_output> growth </growth_output>
        <growth_output> maint </growth_output>
        <growth_output> ag_factor </growth_output>
        <growth_output> bg_factor </growth_output>
        <growth_output> w_r_ag </growth_output>
        <growth_output> w_h_ag </growth_output>
        <growth_output> w_r_bg </growth_output>
        <growth_output> w_h_bg </growth_output>
    </output>
</MangaProject>
